
package domain;

import org.hibernate.validator.constraints.NotBlank;

public class Comment extends DomainEntity {

	// Constructor

	public Comment() {
		super();
	}


	// Attributes

	private String	body;


	@NotBlank
	public String getBody() {
		return this.body;
	}

	public void setBody(final String body) {
		this.body = body;
	}

}
